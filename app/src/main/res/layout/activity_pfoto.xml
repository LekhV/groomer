<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Navigator.Category.Products.DetailProduct.DetailProductsNew.Photos.ActivityPfoto">
    <include
        layout="@layout/toolbar4"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        />
    <me.relex.photodraweeview.PhotoDraweeView
        android:id="@+id/photo_drawee_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="56dp"
        app:failureImage="@drawable/logo_white"
        app:failureImageScaleType="centerInside"
        app:placeholderImage="@drawable/logo_white"
        app:placeholderImageScaleType="fitCenter"
        app:progressBarAutoRotateInterval="1000"
        app:progressBarImage="@drawable/logo_white"
        app:progressBarImageScaleType="centerInside"/>

</RelativeLayout>